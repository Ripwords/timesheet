{
  "name": "@timesheet/server",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "build": "bun --compile --minify-whitespace --minify-syntax --target bun --outfile server ./src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:reset": "bun run src/db/reset.ts",
    "db:seed": "bun run src/db/seedTestData.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/jwt": "^1.2.0",
    "@elysiajs/server-timing": "^1.2.1",
    "@elysiajs/swagger": "^1.2.2",
    "@rasla/logify": "^5.0.0",
    "bcryptjs": "^3.0.2",
    "elysia": "latest",
    "nodemailer": "^6.10.1",
    "pg": "^8.14.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.12",
    "bun-types": "latest",
    "drizzle-seed": "^0.3.1"
  },
  "module": "src/index.ts",
  "main": "src/index.ts"
}